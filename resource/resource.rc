#include"../Include/resouse.hpp"
CREATEPROCESS_MANIFEST_RESOURCE_ID RT_MANIFEST "main.exe.xml"
ID_MENU MENU
BEGIN
    POPUP TEXT("文件")
    BEGIN
        MENUITEM TEXT("新建(&O)\tCtrl+O"),        ID_FILE_NEW
        MENUITEM TEXT("保存(&C)\tCtrl+C"),        ID_FILE_SAVE
        MENUITEM TEXT("导入(&G)\tCtrl+G"),        ID_FILE_READ   
    END
    POPUP TEXT("帮助")
    BEGIN
        MENUITEM TEXT("属性(&A)\tCtrl+A"),        ID_HELP_ATTRIBUTE
        MENUITEM TEXT("关于(&R)\tCtrl+R"),        ID_HELP_ABOUT
        MENUITEM TEXT("快捷键(&S)\tCtrl+S"),      ID_HELP_ACCELERATORS  
    END                    
    POPUP TEXT("程序")
    BEGIN
        MENUITEM TEXT("实现思路(&I)\tCtrl+I"),    ID_CODE_IDEA   
        MENUITEM TEXT("实现工具(&T)\tCtrl+T"),    ID_CODE_TOOL
    END    
END

IDR_ACC  ACCELERATORS
{
    "^O",            ID_FILE_NEW            
    "^C",            ID_FILE_SAVE           
    "^G",            ID_FILE_READ       
    "^A",            ID_HELP_ATTRIBUTE      
    "^R",            ID_HELP_ABOUT    
    "^S",            ID_HELP_ACCELERATORS
    "^I",            ID_CODE_IDEA           
    "^T",            ID_CODE_TOOL
    VK_LEFT,         ID_LEFT,                          VIRTKEY,NOINVERT
    VK_RIGHT,        ID_RIGHT,                         VIRTKEY,NOINVERT
    VK_UP,           ID_UP,                            VIRTKEY,NOINVERT
    VK_DOWN,         ID_DOWN,                          VIRTKEY,NOINVERT
    VK_SPACE,        ID_SPACE,                         VIRTKEY,NOINVERT
    VK_RETURN,       ID_RETURN,                        VIRTKEY,NOINVERT
    VK_HOME,         ID_H_HOME,                        VIRTKEY,NOINVERT
    VK_HOME,         ID_V_HOME,                        VIRTKEY,SHIFT,NOINVERT
    VK_END,          ID_H_END,                         VIRTKEY,NOINVERT  
    VK_END,          ID_V_END,                         VIRTKEY,SHIFT,NOINVERT                                 
}

