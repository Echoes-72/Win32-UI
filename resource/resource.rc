CAPTION "实现工具"
FONT 10, "MS Shell Dlg", 400, 0, 0x1
BEGIN
    CONTROL TOOL, 18181, TEXT("Static"), SS_LEFT, 5, 5, 250, 125
    PUSHBUTTON TEXT("确认"), IDOK, 45 , 50 , 50 , 15   
END

